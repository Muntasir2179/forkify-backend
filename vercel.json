{
    "version": 2,
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python3.11.0"
      },
      {
        "src": "forkify_backend/wsgi.py",
        "use": "@vercel/python3.11.0"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "forkify_backend/wsgi.py"
      }
    ]
  }